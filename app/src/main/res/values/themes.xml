<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Evaluacion" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#1c268e</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <!-- Customize your theme here. -->
    </style>


    <style name="Theme.Evaluacion.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="Theme.Evaluacion.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Evaluacion.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Theme.FootLoose.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.FootLoose.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.FootLoose.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>